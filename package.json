  "version": "0.2.1",
  "homepage": "https://github.com/jonschlinkert/base-cli",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/base-cli",
  "bugs": {
    "url": "https://github.com/jonschlinkert/base-cli/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "isobject": "^2.0.0",
    "lazy-cache": "^0.2.4",
    "map-config": "^0.2.1",
    "resolve-dir": "^0.1.0"
  },
  "devDependencies": {
    "base-data": "^0.3.3",
    "base-methods": "^0.3.1",
    "base-options": "^0.4.2",
    "base-plugins": "^0.3.0",
    "base-store": "^0.1.0",
    "expand-args": "^0.2.1",
    "gulp": "^3.9.0",
    "gulp-istanbul": "^0.10.2",
    "gulp-jshint": "^1.11.2",
    "gulp-mocha": "^2.1.3",
    "jshint-stylish": "^2.0.1",
    "minimist": "^1.2.0",
    "mocha": "*"
